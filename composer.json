{
    "name": "skeeks/cms-db-dumper",
    "description": "Database dumper for SkeekS CMS",
    "keywords": [
        "yii",
        "skeeks",
        "framework",
        "component",
        "db",
        "dump"
    ],
    "homepage": "https://cms.skeeks.com/",
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://skeeks.com/",
        "wiki": "https://cms.skeeks.com/docs/",
        "source": "https://github.com/skeeks-cms/cms-db-dumper"
    },
    "authors": [
        {
            "name": "Semenov Alexander",
            "email": "semenov@skeeks.com"
        }
    ],
    "require": {
        "skeeks/cms": "^6.0 || dev-master",
        "ifsnop/mysqldump-php": "^2.4.0"
    },
    "autoload": {
        "psr-4": {
            "skeeks\\cms\\dbDumper\\": "src/"
        }
    },
    "extra": {
        "config-plugin": {
            "web": [
                "src/config/common.php",
                "src/config/web.php"
            ],
            "console": [
                "src/config/common.php",
                "src/config/console.php"
            ]
        }
    }
}
